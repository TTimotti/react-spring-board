<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper3.0//EN" "http://mybatis.org/schema/mybatis-3-mapper.dtd">

<mapper namespace="study.work.signAccess.mapper.MainMapper">
    <select id="selectData" resultType="TbUserDao">
        SELECT
            id,
            title,
            content,
            created_dt,
            author_id
        FROM
            tb_board
        WHERE
            id = 1
    </select>
</mapper>